<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>ServletTopMenu</servlet-name>
		<servlet-class>com.anusha.portfolio.codelab.ServletTopMenu</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletTopMenu</servlet-name>
		<url-pattern>/topMenu</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ServletSubMenu</servlet-name>
		<servlet-class>com.anusha.portfolio.codelab.ServletSubMenu</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletSubMenu</servlet-name>
		<url-pattern>/subMenu</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ServletPicture</servlet-name>
		<servlet-class>com.anusha.portfolio.codelab.ServletPicture</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletPicture</servlet-name>
		<url-pattern>/picture</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

		
  <!--  This is required after the portfolio is generated -->		
  <servlet>
    <servlet-name>greetServlet</servlet-name>
    <servlet-class>com.anusha.portfolio.server.GreetingServiceImpl</servlet-class>
  </servlet>
<!--   <servlet>
    <servlet-name>unaCreationsHomePageServlet</servlet-name>
    <servlet-class>com.anusha.portfolio.server.ControllerServiceImpl</servlet-class>
  </servlet> -->
  
  <servlet-mapping>
    <servlet-name>greetServlet</servlet-name>
    <url-pattern>/portfolio/greet</url-pattern>
  </servlet-mapping>
<!--   <servlet-mapping>
    <servlet-name>unaCreationsHomePageServlet</servlet-name>
    <url-pattern>/unacreations/*</url-pattern>
  </servlet-mapping> -->
		
</web-app>
